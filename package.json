{
  "name": "express-sequelize-api",
  "version": "1.0.0",
  "description": "A command-line application to manage a company's employee database using Node.js, Inquirer, and MySQL.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "seed": "node seeders/seed.js",
    "git-add": "git add .",
    "git-commit": "node -e \"require('child_process').execSync('git commit -m \\\"Updating the repository\\\"', { stdio: 'inherit' })\"",
    "git-push": "node -e \"require('child_process').execSync('git push origin myBranch', { stdio: 'inherit' })\"",
    "git-status": "git status",
    "git-pull": "git pull origin myBranch",
    "git-all": "npm run git-add && npm run git-commit && npm run git-push",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2"
  }
}
